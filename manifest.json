{
    "manifest_version" : 2,
    "name" : "Coronavirus Blocker",
    "version" : "0.1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js" : ["script.js"],
            "css" : ["style.css"]
        }
    ],
    "browser_action":{
       
        "default_icon":{
            "16":"icons/coronavirus16.png",
            "48":"icons/coronavirus48.png"
        },
        "default_title": "coronavirus blocker and highlighter",
        "default_popup":"popup.html"
    },
    "icons":{
        "16":"icons/coronavirus16.png",
        "48":"icons/coronavirus48.png"
    }
    
}